{
    "metadata": {
        "name": "my-target"
    },
    "spec": {
        "forceRedeploy": true,
        "components": [
            {
                "name": "ecu",
                "type": "my_ecu_type",
                "properties": {
                    "package": "my_ecu_package",
                    "break": "0",
                    "url": "localhost"
                }
            }
        ],
        "topologies": [
            {
                "bindings": [
                    {
                        "role": "my_ecu_type", 
                        "provider": "providers.target.mqtt",
                        "config": {
                            "name": "proxy",
                            "brokerAddress": "tcp://10.21.220.194:1883",
                            "clientID": "symphony",
                            "requestTopic": "coa-request",
                            "responseTopic": "coa-response",
                            "timeoutSeconds":  "30"
                        }
                    }
                ]
            }
        ]
    }
}